{
    "manifest_version": 3,
    "name": "FocusTube",
    "version": "1.0",
    "description": "Hide the YouTube homepage feed and Shorts section.",
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "content_scripts": [
      {
        "matches": [
          "https://www.youtube.com/*"
        ],
        "css": [
          "styles.css"
        ],
        "js": [
          "content.js"
        ]
      }
    ]
  }
